class TimeParameterization:
    def getName(self) -> str: ...

    def compute(self, composite_instruction: tesseract_robotics.tesseract_command_language._tesseract_command_language.CompositeInstruction, env: "tesseract_environment::Environment", profiles: tesseract_robotics.tesseract_command_language._tesseract_command_language.ProfileDictionary) -> bool: ...

class TimeOptimalTrajectoryGeneration(TimeParameterization):
    def __init__(self, name: str = 'TOTG') -> None: ...

class IterativeSplineParameterization(TimeParameterization):
    def __init__(self, name: str = 'ISP') -> None: ...
