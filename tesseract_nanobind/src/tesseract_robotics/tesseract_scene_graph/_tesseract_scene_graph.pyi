class SceneGraph:
    def __init__(self, name: str = "") -> None: ...
    def clone(self) -> SceneGraph: ...
    def clear(self) -> None: ...
    def setName(self, name: str) -> None: ...
    def getName(self) -> str: ...
    def setRoot(self, name: str) -> bool: ...
    def getRoot(self) -> str: ...
    @overload
    def addLink(self, link: Link, replace_allowed: bool = False) -> bool: ...
    @overload
    def addLink(self, link: Link, joint: Joint) -> bool: ...
    def getLink(self, name: str) -> Link: ...
    def getLinks(self) -> list[Link]: ...
    def getLeafLinks(self) -> list[Link]: ...
    def removeLink(self, name: str, recursive: bool = False) -> bool: ...
    def moveLink(self, joint: Joint) -> bool: ...
    def setLinkVisibility(self, name: str, visibility: bool) -> None: ...
    def getLinkVisibility(self, name: str) -> bool: ...
    def setLinkCollisionEnabled(self, name: str, enabled: bool) -> None: ...
    def getLinkCollisionEnabled(self, name: str) -> bool: ...
    def addJoint(self, joint: Joint) -> bool: ...
    def getJoint(self, name: str) -> Joint: ...
    def removeJoint(self, name: str, recursive: bool = False) -> bool: ...
    def moveJoint(self, name: str, parent_link: str) -> bool: ...
    def getJoints(self) -> list[Joint]: ...
    def getActiveJoints(self) -> list[Joint]: ...
    def changeJointOrigin(
        self, name: str, new_origin: "Eigen::Transform<double, 3, 1, 0>"
    ) -> bool: ...
    def changeJointLimits(self, name: str, limits: JointLimits) -> bool: ...
    def changeJointPositionLimits(self, name: str, lower: float, upper: float) -> bool: ...
    def changeJointVelocityLimits(self, name: str, limit: float) -> bool: ...
    def changeJointAccelerationLimits(self, name: str, limit: float) -> bool: ...
    def changeJointJerkLimits(self, name: str, limit: float) -> bool: ...
    def getJointLimits(self, name: str) -> JointLimits: ...
    def addAllowedCollision(self, link_name1: str, link_name2: str, reason: str) -> None: ...
    @overload
    def removeAllowedCollision(self, link_name1: str, link_name2: str) -> None: ...
    @overload
    def removeAllowedCollision(self, link_name: str) -> None: ...
    def clearAllowedCollisions(self) -> None: ...
    def isCollisionAllowed(self, link_name1: str, link_name2: str) -> bool: ...
    def getAllowedCollisionMatrix(self) -> "tesseract_common::AllowedCollisionMatrix":
        """Get the allowed collision matrix"""

    def getSourceLink(self, joint_name: str) -> Link: ...
    def getTargetLink(self, joint_name: str) -> Link: ...
    def getInboundJoints(self, link_name: str) -> list[Joint]: ...
    def getOutboundJoints(self, link_name: str) -> list[Joint]: ...
    def isAcyclic(self) -> bool: ...
    def isTree(self) -> bool: ...
    def isEmpty(self) -> bool: ...
    def getAdjacentLinkNames(self, name: str) -> list[str]: ...
    def getInvAdjacentLinkNames(self, name: str) -> list[str]: ...
    def getLinkChildrenNames(self, name: str) -> list[str]: ...
    def getJointChildrenNames(self, name: str) -> list[str]: ...
    def getShortestPath(self, root: str, tip: str) -> ShortestPath: ...
    def saveDOT(self, path: str) -> None: ...
    @overload
    def insertSceneGraph(self, scene_graph: SceneGraph, prefix: str = "") -> bool: ...
    @overload
    def insertSceneGraph(self, scene_graph: SceneGraph, joint: Joint, prefix: str = "") -> bool: ...
    def __eq__(self, arg: SceneGraph, /) -> bool: ...
    def __ne__(self, arg: SceneGraph, /) -> bool: ...
    def __repr__(self) -> str: ...
