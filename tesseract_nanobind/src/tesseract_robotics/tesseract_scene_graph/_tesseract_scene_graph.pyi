import enum
from collections.abc import Sequence
from typing import Annotated, overload

import numpy
from numpy.typing import NDArray

import tesseract_robotics.tesseract_geometry._tesseract_geometry

class JointType(enum.Enum):
    UNKNOWN = 0

    REVOLUTE = 1

    CONTINUOUS = 2

    PRISMATIC = 3

    FLOATING = 4

    PLANAR = 5

    FIXED = 6

JointType_UNKNOWN: JointType = JointType.UNKNOWN

JointType_REVOLUTE: JointType = JointType.REVOLUTE

JointType_CONTINUOUS: JointType = JointType.CONTINUOUS

JointType_PRISMATIC: JointType = JointType.PRISMATIC

JointType_FLOATING: JointType = JointType.FLOATING

JointType_PLANAR: JointType = JointType.PLANAR

JointType_FIXED: JointType = JointType.FIXED

class JointDynamics:
    @overload
    def __init__(self) -> None: ...
    @overload
    def __init__(self, damping: float, friction: float) -> None: ...
    @property
    def damping(self) -> float: ...
    @damping.setter
    def damping(self, arg: float, /) -> None: ...
    @property
    def friction(self) -> float: ...
    @friction.setter
    def friction(self, arg: float, /) -> None: ...
    def clear(self) -> None: ...
    def __eq__(self, arg: JointDynamics, /) -> bool: ...
    def __ne__(self, arg: JointDynamics, /) -> bool: ...
    def __repr__(self) -> str: ...

